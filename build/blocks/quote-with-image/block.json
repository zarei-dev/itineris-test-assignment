{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "itineris-assignment/quote-with-image",
  "version": "0.1.0",
  "title": "Quote With Image",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": "h4"
    },
    "mediaID": {
      "type": "number"
    },
    "mediaURL": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "qouteBody": {
      "type": "string",
      "source": "html",
      "selector": ".qouteBody"
    },
    "qouteSource": {
      "type": "string",
      "source": "html",
      "selector": ".qouteSource"
    }
  },
  "textdomain": "quote-with-image",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}